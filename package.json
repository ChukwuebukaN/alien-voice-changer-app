{
  "name": "AlienVoiceChanger",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "apk": "react-native run-android --mode=release",
    "a": "react-native run-android",
    "a-c": "cd android && chmod +x gradlew && ./gradlew clean && cd .. && npm run a",
    "i": "react-native run-ios --simulator='iPhone 15 Pro'",
    "i-c": "cd ios && rm -rf Pods && pod install && cd .. && react-native run-ios --simulator='iPhone 15 Pro'",
    "start": "react-native start",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@react-native/metro-config": "^0.73.2",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^14.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-native-svg-transformer": "^1.2.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
